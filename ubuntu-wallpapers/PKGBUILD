pkgname=ubuntu-wallpapers
pkgver=0.34.0
pkgrel=100
pkgdesc="The default wallpapers for Ubuntu (from 9.10 Karmic Koala to 12.04 Precise Pangolin)"
arch=('any')
url="https://launchpad.net/ubuntu-wallpapers"
license=('GPL')
groups=('unity-extra')
makedepends=('libxslt' 'python-distutils-extra')
install=${pkgname}.install
source=("https://launchpad.net/ubuntu/+archive/primary/+files/${pkgname}_${pkgver}.tar.gz")
sha512sums=('fb36aed9c69cb2070124d2ab39e45687e6b21ebd39973a8c774dc4c77b3e6add40af56a47720b3e43c3587fbbc3a9584670037d50489eab214cf55c17beb8594')

package() {
  cd "${srcdir}/${pkgname}-${pkgver}"
  python2 setup.py install --root="${pkgdir}/" --optimize=1
}

